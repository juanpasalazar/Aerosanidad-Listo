        <%= f.text_field :b_menarquia, class: 'form-control', placeholder: 'Menarquia', :id => "menarquia"  %>
      </div>




      <h5>Esta en Embarazo?</h5>

      <div class="col-md-2">
        <label class="label" for="firstname">Si</label>
         <%= f.check_box :b_embarazo, :id => "yes" %>
      </div>
      <div class="col-md-2" style="margin-left:-30px;">
        <label class="label" for="firstname">No</label>
         <%= f.check_box :b_embarazo, :id => "no" %>
      </div>


