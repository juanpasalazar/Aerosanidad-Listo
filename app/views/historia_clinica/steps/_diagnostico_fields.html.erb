
<div class="nested-fields">

	<div class="row">




<%= f.select :cie10_id,
  Cie10.all.map { |u| ["#{u.id}, #{u.familia}, #{u.codigo}, #{u.descripcion}", u.id] },
               { include_blank: true },
               { class: 'chosen-select' } %>






<script type="text/javascript">
  $(function() {
  return $('.chosen-select').chosen({
    allow_single_deselect: true,
    no_results_text: 'No results matched',
    width: '200px'
  });
});

</script>


	</div>

  <div class="modal-footer">

       <%= link_to_remove_association 'Eliminar', f, :class => "
       btn btn-danger color_blanco custom" %>
    </div>

</div>



