
<div class="container">    
    <div id="loginbox" class="mainbox col-md-12 col-md-offset-1 col-sm-12 col-sm-offset-1"> 
        <div class="panel panel-default" >
            <div class="panel-heading col-md-12">
                <div class="panel-title text-center">Historia Personal 


<button><a href="javascript:history.back()">Historia Clinica</a> </button>


<script>
function goBack() {
    window.history.go(+1);
}
</script>
<button onclick="goBack()">Revision Por Sistemas</button>


            </div>     
            <div class="panel-body" >

<%= simple_form_for [@clinic_history,  @personal_history], :html => { :class => 'form-horizontal' } do |f| %>

                <%= render 'shared/errors' %>

                  <div class="container">
                      <div class="login-container">
                      <div id="output"></div>
                        <div class="avatar"></div>
                        <div class="form-box">


                          <style type="text/css">
                          input, input[placeholder]{
                            font-weight:bold;
                              color:black;
                              text-align:center;

                          }
                          </style>
                          <br/><br/>


                            <div class="row">
                              <div class="col-md-6">   
                                <div class="form-group">
                                  <%= f.text_field :neunatales, class: 'form-control', required: true, placeholder: 'Neonatales' %>
                                </div>
                              </div>
                              <div class="col-md-6">   
                                <div class="form-group">
                                  <%= f.text_field :neunatales_description, class: 'form-control', required: true, placeholder: 'Neonatales Descripción' %>
                                </div>
                              </div>
                            </div>

                            <div class="row">
                              <div class="col-md-6">   
                                <div class="form-group">
                                  <%= f.text_field :patologicos, class: 'form-control', required: true, placeholder: 'Patologicos' %>
                                </div>
                              </div>
                              <div class="col-md-6">   
                                <div class="form-group">
                                  <%= f.text_field :patologicos_description, class: 'form-control', required: true, placeholder: 'Patologicos Description' %>
                                </div>
                              </div>
                            </div>

      

                            <div class="row">
                              <div class="col-md-6">   
                                <div class="form-group">
                                  <%= f.text_field :quirurgicos, class: 'form-control', required: true, placeholder: 'Quirurgicos' %>
                                  <%= f.text_field :quirurgicos_description, class: 'form-control', required: true, placeholder: 'Quirurgicos Descripción',  id: 'Aeropuerto'  %>
                                </div>
                              </div>




                              <div class="col-md-6">   
                                <div class="form-group">
                                  <%= f.text_field :farmacologicos, class: 'ng-binding form-control', required: true, placeholder: 'farmacologicos' %>
                                </div>
                              </div>
                            </div>

                            <div class="row">
                              <div class="col-md-6">   
                                <div class="form-group">
                                  <%= f.text_field :farmacologicos_description, class: 'form-control', required: true, placeholder: 'Farmacologicos Descripción' %>
                                </div>
                              </div>
                              <div class="col-md-6">   
                                <div class="form-group">
                                  <%= f.text_field :alergicos, class: 'form-control', required: true, placeholder: 'alergicos' %>
                                </div>
                              </div>
                            </div>
                            <div class="row">
                              <div class="col-md-6">   
                              <div class="form-group">
                                  <%= f.text_field :alergicos_description, class: 'form-control', required: true, placeholder: 'alergicos_description' %>
                              </div>
                             
                              <div class="col-md-6">   
                              <div class="form-group">
                                <%= f.text_field :toxicos, class: 'form-control', required: true, placeholder: 'toxicos_description' %>
                              </div><br/><br/>
                              </div>
                            </div>
                            </div>
                            <br/>
                            <div class="row">
                              <div class="col-md-6"> 
                              <%= f.submit 'Enviar!', class: 'btn btn-info btn-block login' %>
                              </div>
                            </div>
                        </div>
                      </div>
                  </div>
				<% end %>
            </div>                     
        </div>  
    </div>
</div>




<style type="text/css">
  body {
    background-color: white;
}

#loginbox {
    margin-top: 30px;
}

#loginbox > div:first-child {        
    padding-bottom: 10px;    
}

.iconmelon {
    display: block;
    margin: auto;
}

#form > div {
    margin-bottom: 25px;
}

#form > div:last-child {
    margin-top: 10px;
    margin-bottom: 10px;
}

.panel {    
    background-color: transparent;
}

.panel-body {
    padding-top: 30px;
    background-color: rgba(2555,255,255,.3);
}

#particles {
    width: 100%;
    height: 100%;
    overflow: hidden;
    top: 0;                        
    bottom: 0;
    left: 0;
    right: 0;
    position: absolute;
    z-index: -2;
}

.iconmelon,
.im {
  position: relative;
  width: 150px;
  height: 150px;
  display: block;
  fill: #525151;
}

.iconmelon:after,
.im:after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
</style>











