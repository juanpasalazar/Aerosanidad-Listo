

<%= form_for([@paciente, @historia_clinica]) do |f| %>



  <% if f.object.errors.any? %>
    <div class="error_messages">
      <% f.object.errors.full_messages.each do |error| %>
        <p><%= error %></p>
      <% end %>
    </div>
  <% end %>

  <fieldset>
    <legend>Pet Characteristics</legend>

    <div>
      <%= f.label :auxiliar %>
      <br />
      <%= f.text_area :auxiliar, rows: 10 %>
    </div>

    <div>
      <%= f.label :colour%>
              <%= f.collection_select :aeropuerto_id, Aeropuerto.all, :id, :nombre, {prompt: "Campo Obligatorio"}, {class: "form-control"} %>
    </div>

    <div>
      <%= f.submit 'Next Step' %>
    </div>
  </fieldset>
<% end %>
