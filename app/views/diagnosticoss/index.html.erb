<p id="notice"><%= notice %></p>

<h1>Listing Diagnosticos</h1>

<table>
  <thead>
    <tr>
      <th>Familia</th>
      <th>Codigo</th>
      <th>Simbolo</th>
      <th>Descripcion</th>
      <th>Codigo</th>
      <th>Historia clinica</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diagnosticos.each do |diagnostico| %>
      <tr>
        <td><%= diagnostico.familia %></td>
        <td><%= diagnostico.codigo %></td>
        <td><%= diagnostico.simbolo %></td>
        <td><%= diagnostico.descripcion %></td>
        <td><%= diagnostico.codigo %></td>
        <td><%= diagnostico.historia_clinica %></td>
        <td><%= link_to 'Show', diagnostico %></td>
        <td><%= link_to 'Edit', edit_diagnostico_path(diagnostico) %></td>
        <td><%= link_to 'Destroy', diagnostico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diagnostico', new_diagnostico_path %>
